<h1>Maps#index</h1>
<p>Find me in app/views/maps/index.html.erb</p>

<% if @maps.any? %>
  <table border="1", width="152%">
    <tr>
      <th>name</th>
      <th>status</th>
      <th>language</th>
     
    </tr>
    
    <% @maps.each do |map| %>
      <tr>
        <td><%= link_to map.name, map_path(map) %></td>
        <td><%= map.status %></td>
        <td><%= map.language %></td>
        
      </tr>
    <% end %>
    
  </table>
<% end %>

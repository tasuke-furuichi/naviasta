<ul class="media-list">
  
  <table border="1", width="100%">
    <tr>
      <th>name</th>
      <th>status</th>
      <th>language</th>
      
    </tr>
    
    <% maps.each do |map| %>
      <tr>
        <td><%= link_to map.name, map_path(map) %></td>
        <td><%= map.status %></td>
        <td><%= map.language %></td>
       
      </tr>
    <% end %>
    
  </table>
 
  
</ul>